	<!--Slide 2-->
	<component id="ID_s2_title">
		<text><![CDATA[<b></b>]]></text>
	</component>
	<component id="ID_s2_SC01" fontSize="12" fontFace="_typewriter" fontColor="#ffffff">
		<feedback id="fb"><![CDATA[<b><font face="_sans" color="#FFFFFF">***You must enter the exact and full command. Commands that include a name or password are case-sensitive.***</font></b>
	]]></feedback>
		<existingcommands><![CDATA[<font face="_sans"><b>Enter configuration mode and set FastEthernet0/1 duplex to full and speed 100.</b></font>]]></existingcommands>
		<command prompt="S1#">
			<correctresponse><![CDATA[configure terminal]]></correctresponse>
			<deviceresponse><![CDATA[Enter configuration commands, one per line.  End with CNTL/Z.]]></deviceresponse>
		</command>
		<command prompt="S1(config)#">
			<correctresponse><![CDATA[interface FastEthernet0/1]]></correctresponse>
		</command>
		<command prompt="S1(config-if)#">
			<correctresponse><![CDATA[duplex full]]></correctresponse>
		</command>
		<command prompt="S1(config-if)#">
			<correctresponse><![CDATA[speed 100]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>End out of configuration mode and save the configuration to NVRAM.</b></font>]]></instruction>
		</command>
		<command prompt="S1(config-if)#">
			<correctresponse><![CDATA[end]]></correctresponse>
			<deviceresponse><![CDATA[S1#<br>%SYS-5-CONFIG_I: Configured from console by console]]></deviceresponse>
		</command>
		<command prompt="S1#">
			<correctresponse><![CDATA[copy running-config startup-config]]></correctresponse>
			<instruction><![CDATA[<font face="_sans"><b>You successfully configured the switch port duplex and speed settings.</b></font>]]></instruction>
		</command>
		<command prompt=""/>
	</component>
</textcomponents>
