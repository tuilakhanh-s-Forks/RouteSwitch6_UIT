    <text id="ID_router_txt1">
        <![CDATA[<table width="410" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><strong>ip nat inside source</strong> {<strong>static</strong> {<strong>tcp</strong> | <strong>udp</strong> <i>local-ip local-port</i></td>
  </tr>
  <tr>
    <td><i>global-ip global-port</i>} [<strong>extendable</strong>] </td>
  </tr>
</table>]]>
    </text>
    <text id="ID_router_txt2">
        <![CDATA[R2(config)# <strong><b>ip nat inside source static tcp 192.168.10.254 80 209.165.200.225 8080</b></strong>]]></text>
    <text id="ID_router_txt3">
        <![CDATA[R2(config)# <strong><b>interface Serial0/0/0</b></strong><br/>
R2(config-if)# <strong><b>ip nat inside</b></strong>]]></text>
    <text id="ID_router_txt4">
        <![CDATA[R2(config)# <strong><b>interface Serial0/1/0</b></strong><br/>
R2(config-if)# <strong><b>ip nat outside</b></strong>]]></text>
