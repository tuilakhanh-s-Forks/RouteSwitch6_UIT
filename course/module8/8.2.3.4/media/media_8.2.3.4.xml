	<component id="ID_s3_title">
		<!--text><![CDATA[<b>DHCPv6 Relay Agent</b>]]></text-->
	</component>
	<component id="ID_s3_SC5" fontSize="12" fontFace="_typewriter" fontColor="#ffffff">
		<feedback id="fb"><![CDATA[<b><font face="_sans" color="#FFFFFF">***You must enter the exact and full command. Commands that include a name or password are case-sensitive.***</font></b>
		]]></feedback>
		<existingcommands><![CDATA[<font face="_sans"><b>Configure the DHCPv6 relay commands on the correct router for PC3 to receive an IPv4 address and other parameters from the DHCPv6 server. Exit configuration mode on any device that needs no configuration.</b></font>]]></existingcommands>
		<command prompt="R1(config)#">
			<correctresponse><![CDATA[exit]]></correctresponse>
		</command>
		<command prompt="R2(config)#">
			<correctresponse><![CDATA[exit]]></correctresponse>
		</command>
		<command prompt="R3(config)#">
			<correctresponse><![CDATA[interface g0/0]]></correctresponse>
		</command>
		<command prompt="R3(config-if)#">
			<correctresponse><![CDATA[ipv6 dhcp relay destination 2001:db8:cafe:1::6]]></correctresponse>
			<deviceresponse><![CDATA[<span class="command">R3(config-if)#</span>]]></deviceresponse>
			<instruction><![CDATA[<font face="_sans" class="syntaxBottomLine"><b>You successfully configured the DHCPv6 Relay.</b></font>]]></instruction>
		</command>
		<command prompt=""/>
