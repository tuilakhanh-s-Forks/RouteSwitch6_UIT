		<feedback id="fb"><![CDATA[<b>***</b><font face="_sans" color="#FFFFFF" >You must enter the exact and full command.</font><b>***</b> 
		]]></feedback>
		<existingcommands><![CDATA[<font face="_sans" ><b>Configure the GigabitEthernet 0/0 interface with the IPv6 address 2001:db8:acad:4::1/64. Describe the link as 'Link to LAN 3' and activate the interface.</b></font>]]></existingcommands>
		<command prompt="R2(config)#">
			<correctresponse><![CDATA[interface gigabitethernet 0/0]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[ipv6 address 2001:db8:acad:4::1/64]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[description Link to LAN 3]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[no shutdown]]></correctresponse>
			<deviceresponse><![CDATA[%LINK-5-CHANGED: Interface GigabitEthernet0/0, changed state to up<br>%LINEPROTO-5-UPDOWN: Line protocol on Interface GigabitEthernet0/0, changed state to up<br/>]]></deviceresponse>
			<instruction><![CDATA[<font face="_sans"><b>Configure the GigabitEthernet 0/1 interface with the IPv6 address 2001:db8:acad:5::1/64. Describe the link as 'Link to LAN 4' and activate the interface.</b></font>]]></instruction>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[interface gigabitethernet 0/1]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[ipv6 address 2001:db8:acad:5::1/64]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[description Link to LAN 4]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[no shutdown]]></correctresponse>
			<deviceresponse><![CDATA[<font >%LINK-5-CHANGED: Interface GigabitEthernet0/1, changed state to up<br>%LINEPROTO-5-UPDOWN: Line protocol on Interface GigabitEthernet0/1, changed state to up<br></font>]]></deviceresponse>
			<instruction><![CDATA[<font face="_sans"><b>Configure the Serial 0/0/0 interface with the IPv6 address 2001:db8:acad:3::2/64. Describe the link as 'Link to R1' and activate the interface.</b></font>]]></instruction>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[interface Serial 0/0/0]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[ipv6 address 2001:db8:acad:3::2/64]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[description Link to R1]]></correctresponse>
		</command>
		<command prompt="R2(config-if)#">
			<correctresponse><![CDATA[no shutdown]]></correctresponse>
			<deviceresponse><![CDATA[%LINK-5-CHANGED: Interface Serial0/0/0, changed state to up<br>%LINEPROTO-5-UPDOWN: Line protocol on Interface Serial0/0/0, changed state to up<br><br>]]></deviceresponse>
			<instruction><![CDATA[<font face="_sans" class="syntaxBottomLine"><b>You successfully configured the R2 IPv6 Router interfaces.</b></font>]]></instruction>
		</command>
