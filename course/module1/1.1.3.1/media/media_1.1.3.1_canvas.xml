    <text id="ID_router_txt1">
        <![CDATA[
	Router#<b> configure terminal</b><br/>
	Enter configuration commands, one per line.  End with CNTL/Z.<br/>
	Router(config)#<b> hostname R1</b><br/>
	R1(config)#
]]></text>
    <text id="ID_router_txt2">
        <![CDATA[
	R1(config)#<b> enable secret class</b><br/>
	R1(config)#<br/> 
	R1(config)#<b> line console 0</b><br/>
	R1(config-line)#<b> password cisco</b><br/>
	R1(config-line)#<b> login</b><br/>
	R1(config-line)#<b> exit</b><br/>
	R1(config)#<br/> 
	R1(config)#<b> line vty 0 4</b><br/>
	R1(config-line)#<b> password cisco</b><br/>
	R1(config-line)#<b> login</b><br/>
	R1(config-line)#<b> exit</b><br/>
	R1(config)#<br/> 
	R1(config)#<b> service password-encryption</b><br/>
	R1(config)#
]]></text>
    <text id="ID_router_txt3">
